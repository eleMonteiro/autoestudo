se app>\n        <div class=\"d-flex align-center\">\n          <v-img\n            alt=\"Vuetify Logo\"\n            class=\"shrink mr-2\"\n            contain\n            src=\"/static/images/logo01.png\"\n            transition=\"scale-transition\"\n            width=\"40\"\n          />\n        </div>\n        <v-toolbar-items class=\"ml-3\">\n          <v-btn to=\"/\" text>Início</v-btn>\n        </v-toolbar-items>\n        <v-toolbar-items class=\"ml-3\">\n          <v-btn to=\"/usuarios/listar\" text>Usuario</v-btn>\n        </v-toolbar-items>\n        <v-toolbar-items class=\"ml-3\">\n          <v-btn to=\"/cursos/listar\" text>Curso</v-btn>\n        </v-toolbar-items>\n\n        <v-spacer></v-spacer>\n\n        <span>Olá, {{nome}}</span>\n        <v-btn @click=\"logout\" text>\n          <v-icon>power_settings_new</v-icon>\n        </v-btn>\n      </v-app-bar>\n\n      <v-content class=\"content\">\n        <v-container fluid class=\"page-wrapper\">\n          <router-view></router-view>\n        </v-container>\n      </v-content>\n\n      <!-- App Footer -->\n      <v-footer height=\"auto\" class=\"white pa-3 app--footer\" app>\n        <span\n          class=\"caption\"\n        >Núcleo de Práticas em Informática &copy; {{ new Date().getFullYear() }}</span>\n      </v-footer>\n    </template>\n\n    <template v-else>\n      <transition>\n        <keep-alive>\n          <router-view :key=\"$route.fullpath\"></router-view>\n        </keep-alive>\n      </transition>\n    </template>\n  </v-app>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport auth from \"./store/modules/auth\";\nimport app from \"./store/modules/app\"\n\nexport default {\n  name: \"App\",\n\n\n  data: () => ({\n    nome: ''\n  }),\n\n  created() {\n    this.nome = app.getters.GET_USUARIO()\n  },\n\n  methods: {\n    logout() {\n      this.$store.dispatch(\"auth/logout\")\n      this.$router.push(\"/login\")\n      localStorage.clear()\n    }\n  }\n};\n</script>\n\n<style lang=\"stylus\" scoped>\n.page-wrapper {\n  min-height: calc(100vh - 64px - 50px - 81px);\n}\n\n.content {\n  background-color: #cbcedd;\n}\n</style>\n"]}]}